 function getCustomPanoramaTileUrl(panoID, zoom, tileX, tileY) 
 {
  // Return a pano image given the panoID.
  return "upload/streetview/" + panoID + '-'  + tileX + '-' +tileY + '_s1.jpg';
 }


function getCustomPanorama(panoID) 
{
 var streetViewPanoramaData = 
 {
    
  links: [],
	copyright: 'Link !',
	tiles:
	{
		tileSize: new google.maps.Size(512, 512),
		worldSize: new google.maps.Size(4096, 2048),
		centerHeading: 0,
		getTileUrl: getCustomPanoramaTileUrl
	}
  };

   switch(panoID) 
   {
   	case 'pr-3-pt-12':
	streetViewPanoramaData['location'] = {
	pano:'pr-3-pt-12',
	description:'',
	latLng: new google.maps.LatLng(14.99595,102.11123)
	}
	return streetViewPanoramaData;
	case 'pr-3-pt-13':
	streetViewPanoramaData['location'] = {
	pano:'pr-3-pt-13',
	description:'',
	latLng: new google.maps.LatLng(14.99561,102.11080)
	}
	return streetViewPanoramaData;
   }
}

function createCustomLink() 
{
  /*
   * add links
   */
  var links = streetView.getLinks();
  var panoID = streetView.getPano();

  switch(panoID) 
  {
      case initPosPanoID:
      links.push(
      {
           description : "Enter",
           pano :"pr-3-pt-12",heading :120,      }
      );
      break;

        }

}

